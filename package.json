{
  "name": "ts-library",
  "version": "0.1.4",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "npm run test",
    "build": "rm -rf ./dist/ && rollup -c",
    "test": "jest --watch --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ts-bps/ts-library"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.3",
    "comment-json": "^1.1.3",
    "jest": "^23.6.0",
    "parcel-bundler": "^1.10.1",
    "rollup": "^0.66.4",
    "rollup-plugin-typescript2": "^0.17.1",
    "typescript": "^3.1.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ]
  }
}
