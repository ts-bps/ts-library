{
  "name": "ts-library",
  "version": "0.1.4",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "npm run test",
    "build": "rm -rf ./build/ && rm -rf ./dist/ && tsc && microbundle build/index.js && cp ./build/index.d.ts ./dist/",
    "test": "jest --watch --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ts-bps/ts-library"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "comment-json": "^1.1.3",
    "jest": "^22.4.3",
    "microbundle": "^0.4.4",
    "parcel-bundler": "^1.7.1",
    "typescript": "^2.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ]
  }
}
